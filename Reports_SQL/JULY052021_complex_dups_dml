-- Delete dups with different index

DELETE
FROM daily_log
WHERE c_id IN (WITH x AS (SELECT d1.c_id
                         FROM daily_log d1,
                              daily_log d2
                         WHERE d1.c_name = d2.c_name
                           AND d1.c_id > d2.c_id
                        )
              
              SELECT *
              FROM x 
              ) ;
